{
  "version": "1.2.1",
  "author": "Bener",
  "private": true,
  "license": "MIT",
  "description": "A purely front-end development navigation",
  "homepage": "https://hello-nav.github.io/",
  "keywords": [
    "hello-nav",
    "nav"
  ],
  "scripts": {
    "dev": "vite",
    "dev:lib": "pnpm --filter @hello-nav/model run dev",
    "build": "tsc && vite build",
    "build:lib": "pnpm --filter @hello-nav/model run build",
    "build:ext": "npm run build && node ./scripts/build-ext.mjs",
    "preview": "vite preview",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@hello-nav/model": "workspace:*",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-svg": "^16.1.34"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "less": "^4.2.0",
    "postcss": "^8.4.44",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-glob": "^0.3.2",
    "zx": "^8.1.5"
  },
  "workspaces": [
    "packages/*"
  ]
}
