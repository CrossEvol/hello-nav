{
  "version": "1.2.1",
  "author": "Bener",
  "private": true,
  "license": "MIT",
  "description": "A purely front-end development navigation",
  "homepage": "https://hello-nav.github.io/",
  "keywords": [
    "hello-nav",
    "nav"
  ],
  "scripts": {
    "dev": "vite",
    "dev:lib": "pnpm --filter @hello-nav/model run dev",
    "build": "tsc && vite build",
    "build:lib": "pnpm --filter @hello-nav/model run build",
    "build:ext": "npm run build && node ./scripts/build-ext-posix.mjs",
    "build:ext:win": "npm run build && node ./scripts/build-ext-win.mjs",
    "preview": "vite preview",
    "lint": "eslint ./src --max-warnings=10",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hello-nav/model": "workspace:*",
    "axios": "^1.7.7",
    "dexie": "^4.0.8",
    "dexie-react-hooks": "^1.1.7",
    "jotai": "^2.9.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-svg": "^16.1.34"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@eslint/migrate-config": "^1.3.0",
    "@types/node": "^22.5.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "less": "^4.2.0",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-glob": "^0.3.2",
    "zx": "^8.1.5"
  },
  "workspaces": [
    "packages/*"
  ]
}